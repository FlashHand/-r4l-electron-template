{
  "name": "rys-desktop",
  "version": "3.0.4",
  "description": "",
  "main": "dist/electron/main.js",
  "scripts": {
    "start": "yarn server & yarn main",
    "dev": "cross-env NODE_ENV=dev node .electron-vue/dev-runner.js -i",
    "pre": "cross-env NODE_ENV=pre DEBUG=true node .electron-vue/dev-runner.js -i",
    "prod": "cross-env NODE_ENV=prod node .electron-vue/dev-runner.js -i",
    "base": "cross-env NODE_ENV=prod DEBUG=true node .electron-vue/dev-runner.js --mode=base",
    "server": "cross-env NODE_ENV=dev DEBUG=true node .electron-vue/dev-runner.js --mode=server",
    "build:test2": "cross-env NODE_ENV=build-test2 node .electron-vue/build.js && electron-builder -w",
    "build:dev": "cross-env NODE_ENV=build-dev node .electron-vue/build.js && electron-builder",
    "build_pre": "cross-env NODE_ENV=pre node .electron-vue/build.js && electron-builder --config ./build-configs/default.json5",
    "build": "cross-env NODE_ENV=prod node .electron-vue/build.js && electron-builder --config ./build-configs/default.json5",
    "base_ydz": "cross-env NODE_ENV=prod_ydz DEBUG=true node .electron-vue/dev-runner.js --mode=base",
    "build_ydz": "cross-env NODE_ENV=prod_ydz node .electron-vue/build.js && electron-builder --config ./build-configs/ydz.json5",
    "build:win": "cross-env NODE_ENV=build-prod node .electron-vue/build.js && electron-builder -w",
    "build:mac": "cross-env NODE_ENV=build-prod node .electron-vue/build.js && electron-builder -m",
    "build:dir": "cross-env NODE_ENV=build-prod node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "pack": "npm run pack:main && npm run pack:renderer && npm run pack:preload",
    "pack:main": "cross-env NODE_ENV=build-prod webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "test": "npm run unit && npm run e2e",
    "postinstall": "electron-builder install-app-deps && rig postinstall",
    "preinstall": "rig preinstall"
  },
  "protocols": {
    "name": "RYS App Protocol",
    "schemes": [
      "rys-app"
    ]
  },
